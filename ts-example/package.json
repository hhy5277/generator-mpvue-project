        "webpack-merge": "^4.1.0"
    },
    "engines": { "node": ">= 4.0.0", "npm": ">= 3.0.0" },
    "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"],
    "pre-commit": ["precommit-msg", "lint-staged"],
    "main": "index.js",
    "author": "wuxueqian <wuxueqian2010@hotmail.com>",
    "license": "MIT",
    "ts": "Y",
    "yarn": "Y",
    "appid": ""
}
